<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.playlistmusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlaylistMusic">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".list0_main"></activity>
        <activity android:name=".list1_main"></activity>
        <activity android:name=".list2_main"></activity>
        <activity android:name=".ListSong"></activity>
        <activity android:name=".ui_list0"></activity>
        <activity android:name=".ui_list1"></activity>
        <activity android:name=".ui_list2"></activity>
    </application>

</manifest>